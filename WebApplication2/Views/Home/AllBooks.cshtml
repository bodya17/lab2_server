@model IEnumerable<WebApplication2.Models.Book>
@{
    ViewBag.Title = "AllBooks";
}

<h2>AllBooks</h2>

@foreach (var item in Model)
{
    <div>
        <h4>@item.BookId</h4>
        <h4>@item.Title</h4>
        @Html.ActionLink("Delete", "DeleteBook", "Home", new { bookId = @item.BookId }, null)
        <hr />
    </div>
}
